{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { inject } from 'vue';\nimport { UUserRequest, UQuery, UUserResponse } from \"@/ups_user_pb\";\nexport default {\n  data() {\n    return {\n      orderId: \"\",\n      order: null\n    };\n  },\n  setup() {\n    const socket = inject('$socket');\n    return {\n      socket\n    };\n  },\n  methods: {\n    async queryOrder() {\n      const query = new UQuery();\n      query.setPackageId(this.packageId);\n      query.setUserId(this.userId);\n      const request = new UUserRequest();\n      request.setQueryCommand(query);\n      this.$socket.connect();\n      this.$socket.send(\"queryOrder\", request);\n      console.log(\"query Start!\");\n      this.$socket.onopen = () => {\n        console.log(\"Socket connection successful!\");\n        // 在这里可以进行后续操作，如发送数据等\n      };\n\n      this.$socket.onerror = error => {\n        console.error(\"WebSocket error: \", error);\n        console.log(\"fail\"); // 打印失败信息\n      };\n\n      const binaryPayload = await this.$socket.onMessage(\"queryOrderResponse\");\n      const response = UUserResponse.deserializeBinary(binaryPayload);\n      const packageList = response.getQueryResult().getPackageList();\n      for (let i = 0; i < packageList.length; i++) {\n        console.log(packageList[i]);\n      }\n      this.$router.push(\"/\");\n    }\n  }\n};","map":{"version":3,"names":["inject","UUserRequest","UQuery","UUserResponse","data","orderId","order","setup","socket","methods","queryOrder","query","setPackageId","packageId","setUserId","userId","request","setQueryCommand","$socket","connect","send","console","log","onopen","onerror","error","binaryPayload","onMessage","response","deserializeBinary","packageList","getQueryResult","getPackageList","i","length","$router","push"],"sources":["/Users/bhc/erss-hwk5-sq44-hb147/ups_frontend/src/components/QueryOrder.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>Query Order</h1>\n    <form @submit.prevent=\"queryOrder\">\n      <label for=\"packageId\">Package ID:</label>\n      <input v-model=\"packageId\" type=\"text\" id=\"packageId\" name=\"packageId\" />\n      <br />\n      <label for=\"userId\">User ID:</label>\n      <input v-model=\"userId\" type=\"text\" id=\"userId\" name=\"userId\" />\n      <br />\n      <button type=\"submit\">Query Order</button>\n    </form>\n  </div>\n</template>\n\n\n<script>\nimport { inject } from 'vue';\n\n  import {\n    UUserRequest,\n    UQuery,\n    UUserResponse,\n  } from \"@/ups_user_pb\";\n\nexport default {\n  data() {\n    return {\n      orderId: \"\",\n      order: null,\n    };\n  },\n  setup() {\n    const socket = inject('$socket');\n    return { socket };\n  },\n  methods: {\n    async queryOrder() {\n      const query = new UQuery();\n      query.setPackageId(this.packageId);\n      query.setUserId(this.userId);\n\n      const request = new UUserRequest();\n      request.setQueryCommand(query);\n      this.$socket.connect();\n      this.$socket.send(\"queryOrder\", request);\n\n      console.log(\"query Start!\");\n      this.$socket.onopen = () => {\n          console.log(\"Socket connection successful!\");\n          // 在这里可以进行后续操作，如发送数据等\n      }\n      this.$socket.onerror = (error) => {\n          console.error(\"WebSocket error: \", error);\n          console.log(\"fail\"); // 打印失败信息\n      };\n      const binaryPayload = await this.$socket.onMessage(\"queryOrderResponse\");\n      \n      const response = UUserResponse.deserializeBinary(binaryPayload);\n      const packageList = response.getQueryResult().getPackageList();\n\n      for (let i = 0; i < packageList.length; i++) {\n        console.log(packageList[i]);\n      }\n      this.$router.push(\"/\");\n    },\n  },\n};\n</script>\n"],"mappings":";AAiBA,SAASA,MAAK,QAAS,KAAK;AAE1B,SACEC,YAAY,EACZC,MAAM,EACNC,aAAa,QACR,eAAe;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK