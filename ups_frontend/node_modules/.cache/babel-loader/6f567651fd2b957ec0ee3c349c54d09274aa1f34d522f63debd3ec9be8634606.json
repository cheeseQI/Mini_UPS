{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      packageId: \"\",\n      newX: \"\",\n      newY: \"\"\n    };\n  },\n  methods: {\n    async queryOrderByUsername() {\n      console.log(\"query by username\");\n      try {\n        const response = await axios.get(`http://localhost:8080/query/2/${this.username}`);\n        const packageList = response.data.PackageInfoList;\n        this.$router.push({\n          path: \"/package-list\",\n          query: {\n            packageList: JSON.stringify(packageList)\n          }\n        });\n      } catch (error) {\n        console.error(error);\n        alert(\"redirect failed\");\n      }\n    },\n    async redirectOrder() {\n      try {\n        console.log(this.$route.query.packageId);\n        const response = await axios.post(\"http://localhost:8080/redirect\", {\n          packageId: this.$route.query.packageId,\n          newX: this.newX,\n          newY: this.newY\n        });\n        console.log(response.data);\n        if (response.data.contain(\"success\")) {}\n        Swal.fire({\n          icon: 'success',\n          title: 'Login success',\n          showConfirmButton: false,\n          timer: 1500\n        });\n        this.$router.push(\"/personalCenter\");\n      } catch (error) {\n        console.error(\"Error creating order:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","data","packageId","newX","newY","methods","queryOrderByUsername","console","log","response","get","username","packageList","PackageInfoList","$router","push","path","query","JSON","stringify","error","alert","redirectOrder","$route","post","contain","fire","icon","title","showConfirmButton","timer"],"sources":["/Users/bhc/erss-hwk5-sq44-hb147/ups_frontend/src/components/RedirectOrder.vue"],"sourcesContent":["<template>\n    <div id=\"redirect-order\">\n        <h1>Redirect Order</h1>\n        <form @submit.prevent=\"redirectOrder\">\n            <div class=\"form-field\">\n                <label for=\"packageId\">Package ID: {{$route.query.packageId}}</label>\n            </div>\n            <div class=\"form-field\">\n                <label for=\"description\">Description: {{$route.query.description}}</label>\n            </div>\n            <div class=\"form-field\">\n                <label for=\"newX\">New X:</label>\n                <input v-model=\"newX\" type=\"text\" id=\"newX\" name=\"newX\" />\n            </div>\n            <div class=\"form-field\">\n                <label for=\"newY\">New Y:</label>\n                <input v-model=\"newY\" type=\"text\" id=\"newY\" name=\"newY\" />\n            </div>\n            <button class=\"action-btn\" type=\"submit\">Redirect Order</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nexport default {\n    data() {\n        return {\n            packageId: \"\",\n            newX: \"\",\n            newY: \"\"\n        };\n    },\n    methods: {\n        async queryOrderByUsername() {\n            console.log(\"query by username\");\n\n            try {\n                const response = await axios.get(\n                    `http://localhost:8080/query/2/${this.username}`\n                );\n                const packageList = response.data.PackageInfoList;\n                this.$router.push({\n                    path: \"/package-list\",\n                    query: {\n                        packageList: JSON.stringify(packageList)\n                    }\n                });\n            } catch (error) {\n                console.error(error);\n                alert(\"redirect failed\");\n            }\n        },\n        async redirectOrder() {\n            try {\n                console.log(this.$route.query.packageId);\n                const response = await axios.post(\"http://localhost:8080/redirect\", {\n                    packageId: this.$route.query.packageId,\n                    newX: this.newX,\n                    newY: this.newY,\n                });\n                console.log(response.data);\n                if(response.data.contain(\"success\")){\n\n                }\n                Swal.fire({\n                    icon: 'success',\n                    title: 'Login success',\n                    showConfirmButton: false,\n                    timer: 1500\n                });\n                this.$router.push(\"/personalCenter\");\n            } catch (error) {\n                console.error(\"Error creating order:\", error);\n            }\n        },\n    },\n};\n</script>\n\n<style scoped>\n#redirect-order {\n    max-width: 600px;\n    padding: 20px;\n    background-color: #ffffff;\n    border-radius: 10px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    margin: 30px auto;\n}\n\nh1 {\n    color: #1e4b88;\n    font-weight: bold;\n    margin-bottom: 20px;\n}\n\n.form-field {\n    margin-bottom: 20px;\n}\n\nlabel {\n    display: block;\n    font-weight: bold;\n    margin-bottom: 5px;\n}\n\ninput {\n    width: 100%;\n    padding: 8px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    font-size: 16px;\n}\n\n.action-btn {\n    background-color: #1e4b88;\n    color: #ffffff;\n    font-size: 16px;\n    padding: 10px 20px;\n    border-radius: 5px;\n    border: none;\n    cursor: pointer;\n    transition: background-color 0.2s;\n}\n\n.action-btn:hover {\n    background-color: #356aa0;\n}\n</style>\n"],"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,oBAAoBA,CAAA,EAAG;MACzBC,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;MAEhC,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAC3B,iCAAgC,IAAI,CAACC,QAAS,GAClD;QACD,MAAMC,WAAU,GAAIH,QAAQ,CAACR,IAAI,CAACY,eAAe;QACjD,IAAI,CAACC,OAAO,CAACC,IAAI,C