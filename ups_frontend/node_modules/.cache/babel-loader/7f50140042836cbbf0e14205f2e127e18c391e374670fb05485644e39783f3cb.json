  selectedPackageId: \"\",\n        };\n    },\n    methods: {\n        getPackageInfo(packageId) {\n            return this.packageList.find(\n                (packageInfo) => packageInfo.packageId === packageId\n            );\n        },\n    },\n    created() {\n        if (this.$route.query.packageList) {\n            this.packageList = JSON.parse(this.$route.query.packageList);\n        }\n    },\n};\n</script>\n"],"mappings":"AA8BA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE,EAAE;MACfC,iBAAiB,EAAE;IACvB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,cAAcA,CAACC,SAAS,EAAE;MACtB,OAAO,IAAI,CAACJ,WAAW,CAACK,IAAI,CACvBC,WAAW,IAAKA,WAAW,CAACF,SAAQ,KAAMA,SAAQ,CACtD;IACL;EACJ,CAAC;EACDG,OAAOA,CAAA,EAAG;IACN,IAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACT,WAAW,EAAE;MAC/B,IAAI,CAACA,WAAU,GAAIU,IAAI,CAACC,KAAK,CAAC,IAAI,CAACH,MAAM,CAACC,KAAK,CAACT,WAAW,CAAC;IAChE;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}