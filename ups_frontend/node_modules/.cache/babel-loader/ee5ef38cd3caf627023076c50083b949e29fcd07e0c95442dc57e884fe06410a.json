{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      packageId: \"\",\n      username: \"\"\n    };\n  },\n  methods: {\n    async queryOrderByPackageId() {\n      console.log(\"query by packageId\");\n      try {\n        const response = await axios.get(`http://localhost:8080/query/1/${this.packageId}`);\n        console.log(response.data.PackageInfoList[0]);\n        const packageInfo = response.data.PackageInfoList[0];\n        this.$router.push({\n          path: \"/package-info\",\n          query: {\n            packageId: packageInfo.packageId,\n            description: packageInfo.description,\n            count: packageInfo.count,\n            destX: packageInfo.destX,\n            destY: packageInfo.destY,\n            username: packageInfo.username,\n            startX: packageInfo.startX,\n            startY: packageInfo.startY,\n            currX: packageInfo.currX,\n            currY: packageInfo.currY\n          }\n        });\n      } catch (error) {\n        console.error(error);\n        alert(\"query failed, please try again later。\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","packageId","username","methods","queryOrderByPackageId","console","log","response","get","PackageInfoList","packageInfo","$router","push","path","query","description","count","destX","destY","startX","startY","currX","currY","error","alert"],"sources":["/Users/bhc/erss-hwk5-sq44-hb147/ups_frontend/src/components/QueryOrderById.vue"],"sourcesContent":["<template>\n    <div id=\"query-order-container\">\n        <div class=\"query-order-form\">\n            <h1>Query Order</h1>\n            <form @submit.prevent=\"queryOrderByPackageId\">\n                <label for=\"packageId\">Package ID:</label>\n                <input v-model=\"packageId\" type=\"text\" id=\"packageId\" name=\"packageId\" />\n                <br />\n                <button class=\"query-btn\" type=\"submit\">Query Order by Package ID</button>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n    data() {\n        return {\n            packageId: \"\",\n            username: \"\",\n        };\n    },\n    methods: {\n        async queryOrderByPackageId() {\n            console.log(\"query by packageId\");\n\n            try {\n                const response = await axios.get(\n                    `http://localhost:8080/query/1/${this.packageId}`\n                );\n                console.log(response.data.PackageInfoList[0]);\n                const packageInfo = response.data.PackageInfoList[0];\n                this.$router.push({\n                    path: \"/package-info\",\n                    query: {\n                        packageId: packageInfo.packageId,\n                        description: packageInfo.description,\n                        count: packageInfo.count,\n                        destX: packageInfo.destX,\n                        destY: packageInfo.destY,\n                        username: packageInfo.username,\n                        startX: packageInfo.startX,\n                        startY: packageInfo.startY,\n                        currX: packageInfo.currX,\n                        currY: packageInfo.currY,\n                    },\n                });\n            } catch (error) {\n                console.error(error);\n                alert(\"query failed, please try again later。\");\n            }\n        },\n    },\n};\n</script>\n\n<style scoped>\n#query-order-container {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n    background-color: #f0f2f5;\n}\n\n.query-order-form {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 350px;\n    padding: 40px;\n    background-color: #ffffff;\n    box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);\n    border-radius: 10px;\n}\n\nh1 {\n    margin-bottom: 30px;\n    font-size: 28px;\n}\n\ninput[type=\"text\"] {\n    width: 100%;\n    padding: 10px;\n    margin: 5px 0 15px 0;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    box-sizing: border-box;\n}\n\n.query-btn {\n    width: 100%;\n    padding: 12px;\n    background-color: #448aff;\n    color: #ffffff;\n    font-size: 18px;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n    transition: background-color 0.2s ease-in-out;\n}\n\n.query-btn:hover {\n    background-color: #2962ff;\n}\n</style>\n"],"mappings":";AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,qBAAqBA,CAAA,EAAG;MAC1BC,OAAO,CAACC,GAAG,CAAC,oBAAoB,CAAC;MAEjC,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAC3B,iCAAgC,IAAI,CAACP,SAAU,GACnD;QACDI,OAAO,CAACC,GAAG,CAACC,QAAQ,CAACP,IAAI,CAACS,eAAe,CAAC,CAAC,CAAC,CAAC;QAC7C,MAAMC,WAAU,GAAIH,QAAQ,CAACP,IAAI,CAACS,eAAe,CAAC,CAAC,CAAC;QACpD,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC;UACdC,IAAI,EAAE,eAAe;UACrBC,KAAK,EAAE;YACHb,SAAS,EAAES,WAAW,CAACT,SAAS;YAChCc,WAAW,EAAEL,WAAW,CAACK,WAAW;YACpCC,KAAK,EAAEN,WAAW,CAACM,KAAK;YACxBC,KAAK,EAAEP,WAAW,CAACO,KAAK;YACxBC,KAAK,EAAER,WAAW,CAACQ,KAAK;YACxBhB,QAAQ,EAAEQ,WAAW,CAACR,QAAQ;YAC9BiB,MAAM,EAAET,WAAW,CAACS,MAAM;YAC1BC,MAAM,EAAEV,WAAW,CAACU,MAAM;YAC1BC,KAAK,EAAEX,WAAW,CAACW,KAAK;YACxBC,KAAK,EAAEZ,WAAW,CAACY;UACvB;QACJ,CAAC,CAAC;MACN,EAAE,OAAOC,KAAK,EAAE;QACZlB,OAAO,CAACkB,KAAK,CAACA,KAAK,CAAC;QACpBC,KAAK,CAAC,uCAAuC,CAAC;MAClD;IACJ;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}